* {
    font:12pt "微软雅黑";
}

QWidget#MainWindow {
	background:transparent;
}

QWidget#systemListWidget {
    background:white;
}

QWidget#systemListScrollArea {
    padding-left: 5px;
    background: white;
    border: 1px solid black;
    border-radius: 4px;
}

QPushButton#openedBtn {
    background-color: rgb(204, 204, 204);
    font-size: 12px;
    border: 1px solid black;
    border-radius: 4px;
}

QPushButton#openedBtn:pressed:hover {
    background-color: rgb(153, 153, 153);
}

QPushButton#openedBtn:hover {
    background-color: rgb(230, 230, 230);
}

QPushButton#openedBtn:disabled {
    background-color: rgb(204, 204, 204);
    color: rgb(122, 122, 122);
}

